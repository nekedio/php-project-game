#!/usr/bin/env php

<?php

define("COUNTSETH", 3); //количество сэтов игры

$autoloadGlob = __DIR__ . '/../../../autoload.php';
$autoloadLoc = __DIR__ . '/../vendor/autoload.php';

if (file_exists($autoloadGlob)) {
    require_once $autoloadGlob;
} else {
    require_once $autoloadLoc;
}

use function BrainGames\games\prime\getDataGamePrime;
use function BrainGames\engine\startEngine;

BrainGames\cli\welcomeGamePrime();

startEngine(getDataGamePrime());